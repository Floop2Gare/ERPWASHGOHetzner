# Configuration Docker Compose pour ERP Wash&Go
# Copiez ce fichier en .docker.env et modifiez les valeurs

# PostgreSQL
POSTGRES_USER=erp_user
POSTGRES_PASSWORD=changez_moi_avec_un_mot_de_passe_securise
POSTGRES_DB=erp_washgo
POSTGRES_PORT=5432

# Backend
BACKEND_PORT=8000
ENABLE_DEBUG_ROUTES=false

# Frontend (si utilisé)
# FRONTEND_PORT=5173

# URLs et origines autorisées (si nécessaire)
# ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

